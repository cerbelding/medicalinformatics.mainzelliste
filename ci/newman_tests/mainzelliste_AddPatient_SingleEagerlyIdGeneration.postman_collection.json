		{
			"name": "GET_PATIENT: check if configured Ids was eagerly generated",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "47020473-8d23-492a-8656-637fc5e64330",
						"exec": [
							"var jsonBody = JSON.parse(responseBody);",
							"console.log(jsonBody);",
							"",
							"tests[\"patient found\"] = responseCode.code === 200;",
							"",
							"tests[\"is patient exportId eagerly generated (configured with eager = pid)?\"] = jsonBody[0].ids.filter(id => id.idType === \"exportId\" && id.idString !== null);",
							"tests[\"is patient localId eagerly generated (configured with eager = *)?\"] = jsonBody[0].ids.filter(id => id.idType === \"localId\" && id.idString !== null);"
						],
						"type": "text/javascript"
					}
				}
